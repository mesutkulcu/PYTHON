      "source": [
        "def prime_number(n):\n",
        "  prime_number = True\n",
        "  for i in range(2,n):\n",
        "    if n == 2:\n",
        "      prime_number = True\n",
        "    elif n%i == 0:\n",
        "      prime_number = False\n",
        "  return prime_number\n",
        "\n",
        "def prime(n):\n",
        "  return [i for i in range(2,n) if prime_number(i)]\n",
        "\n",
        "print(prime(100))"
      ],
